// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int             @unique @default(autoincrement())
}

model Vehicle {
  id        Int             @unique @default(autoincrement())
}

model FuelType {
  id        Int             @unique @default(autoincrement())
}

model FuelUpdate {
  id        Int             @unique @default(autoincrement())
}

model ServiceUpdate {
  id        Int             @unique @default(autoincrement())
}